{
  "extends": "airbnb",
  "env": {
    "node": true,
    "browser": true
  },
  "rules": {
    "arrow-body-style": [0],
    "comma-dangle": 0,
    "no-console": 0,
    "react/prop-types": 0,
    "react/jsx-indent-props": [2, "tab"],
    "max-len": [2, {"code": 130}],
    "globals": [0, {
      "__DEVELOPMENT__": false,
      "__CLIENT__": false,
      "__SERVER__": false,
      "__DEVTOOLS__": false
    }]
  }
}
