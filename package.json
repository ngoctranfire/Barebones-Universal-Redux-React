    "redux-observable": "^0.7.0",
    "redux-thunk": "^2.1.0",
    "rxjs": "^5.0.0-beta.10",
    "webpack-isomorphic-tools": "^2.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.9.0",
    "babel-runtime": "^6.9.2",
    "better-npm-run": "0.0.9",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "clean-webpack-plugin": "^0.1.9",
    "concurrently": "^2.2.0",
    "eslint": "^3.0.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "gulp": "^3.9.1",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "mocha": "^2.5.3",
    "proxy-middleware": "^0.15.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "strip-loader": "^0.1.2",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-validator": "^2.2.3"
  },
  "scripts": {
    "start": "concurrent -kill-others \"npm run start-prod\" \"npm run start-prod-api\"",
    "watch-client": "better-npm-run watch-client",
    "build": "better-npm-run build",
    "dev": "better-npm-run dev"
  },
  "betterScripts": {
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "HOST": "localhost"
      }
    },
    "dev": {
      "command": "node index.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "HOST": "localhost"
      }
    },
    "build": {
      "command": "webpack -p --progress --display-reasons --display-error-details --config webpack/webpack.prod.config.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production"
      }
    }
  },
  "keywords": [
    "LandForward"
  ],
  "author": "ngoctranfire@gmail.com",
  "license": "ISC"
}
