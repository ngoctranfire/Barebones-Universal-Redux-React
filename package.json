{
  "name": "landforward",
  "version": "0.0.1",
  "description": "Land forward repository",
  "main": "index.js",
  "dependencies": {
    "babel-preset-stage-0": "^6.5.0",
    "express": "^4.14.0",
    "immutable": "^3.8.1",
    "pretty-error": "^2.0.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
